<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="scm.entity.DeptMapper">
		<resultMap type="Dept" id="deptResultMap">
			<id property="deptId" column="dept_id"/>
			<result property="deptName" column="dept_name" />
			<result property="deptAdress" column="dept_address"/>
		</resultMap>
	
	
		<select id="selectDept" parameterType="Integer" resultMap="deptResultMap" >
				
				select*from dept where dept_id=#{deptId}; 
		
		</select>
		<insert id="insertDept" parameterType="Dept">
			insert into dept(dept_name,dept_address)values(#{deptName},#{deptAdress})
		</insert>
	
	
	
	</mapper>
	
	
	
